{
  "name": "iso",
  "version": "1.0.0",
  "description": "Isomorphic JS",
  "repository": "https://github.com/jonwatkins/isomorphic",
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register test/",
    "coverage": "./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel/register test/",
    "compile": "./node_modules/.bin/babel src/app --out-dir ./.srv",
    "webpack": "./node_modules/.bin/webpack",
    "dev-server": "./node_modules/.bin/webpack-dev-server",
    "sass": "grunt sass",
    "build": "npm run clean & npm run compile && npm run webpack && npm run sass",
    "clean": "rm -rf ./.srv & rm -rf ./coverage",
    "start": "./node_modules/.bin/babel-node bin/www",
    "cluster": "./node_modules/.bin/babel-node bin/cluster",
    "start-dev": "npm run dev-server & npm start"
  },
  "author": "Jon Watkins <jonwatkins86@gmail.com",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "history": "1.13.x",
    "react": "^0.13.3",
    "react-document-title": "^2.0.0",
    "react-router": "^1.0.0-rc1",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "bower": "^1.7.1",
    "chai": "^3.3.0",
    "concurrently": "^0.1.1",
    "eslint-plugin-react": "^3.3.2",
    "grunt": "^0.4.5",
    "grunt-contrib-sass": "^0.9.2",
    "istanbul": "^0.4.1",
    "jsdom": "3.1.2",
    "mocha": "^2.3.3",
    "npm": "^3.5.2",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.14.0",
    "zombie": "^4.0.8"
  }
}
